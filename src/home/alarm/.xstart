#!/bin/sh

[[ -f ~/.Xresources ]] && xrdb -merge ~/.Xresources
xset b off
xset s off
xset -dpms
xsetroot -cursor_name left_ptr
# pulseaudio --start
amixer sset Master unmute
amixer sset Master 90%
amixer sset PCM 90%
feh --bg-fill /opt/video_pi/video_pi_desktop.png
# xcompmgr -c &

for f in $HOME/bin/autostart-xorg.d/*; do sh "$f"; done &

dwm
